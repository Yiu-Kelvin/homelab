[kdcdefaults]
    kdc_ports = {{ kerberos_kdc_port }}

[realms]
    {{ kerberos_realm }} = {
        # master keytype: aes256-cts-hmac-sha1-96
        max_life = 10h 0m 0s
        max_renewable_life = 7d 0h 0m 0s
        kdc_ports = {{ kerberos_kdc_port }}
        acl_file = {{ kadm5_acl_path }}
        dict_file = /usr/share/dict/words
        admin_keytab = /etc/krb5kdc/kadm5.keytab
    }
